{
   
        "git.ignoreLimitWarning": true,
        "editor.fontFamily": "Fira Code",
        "editor.fontLigatures": true,
        "editor.cursorStyle": "line-thin",
        "editor.cursorBlinking": "smooth",
        "workbench.fontAliasing": "auto",
        "editor.fontSize": 13.5,
        "editor.smoothScrolling": true,
        "editor.tabSize": 2,
        "window.zoomLevel": 0,
        "editor.lineHeight": 24,
    
  
    
    "files.associations": {
        "*.tcc": "cpp",
        "memory": "cpp",
        "array": "cpp",
        "deque": "cpp",
        "list": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "initializer_list": "cpp",
        "functional": "cpp",
        "atomic": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstdarg": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "exception": "cpp",
        "fstream": "cpp",
        "iosfwd": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "numeric": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "cinttypes": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "typeinfo": "cpp"
    },
    "editor.fontWeight": "600"
}